      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "jsbarcode": "^3.11.5"
  },
  "peerDependencies": {
    "solid-js": "^1.4.2"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.74.1",
    "rollup-preset-solid": "^1.4.0",
    "solid-js": "^1.4.2"
  }
}
