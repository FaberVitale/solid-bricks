{
  "name": "@solid-bricks/barcode",
  "version": "1.0.0",
  "description": "A solid-js component that renders barcodes",
  "homepage": "https://fabervitale.github.io/solid-bricks/examples/barcode-generator",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c",
    "lint:ts": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FaberVitale/solid-bricks",
    "directory": "packages/barcode"
  },
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "barcode",
    "solid",
    "solidjs",
    "solid-js",
    "code128",
    "upc",
    "ean",
    "itf",
    "msi",
    "pharmacode"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "jsbarcode": "^3.11.5"
  },
  "peerDependencies": {
    "solid-js": "^1.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "jsdom": "^19.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.74.1",
    "rollup-preset-solid": "^1.4.0",
    "solid-js": "^1.4.2",
    "solid-testing-library": "^0.3.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6",
    "vitest": "^0.13.1"
  }
}
