{
  "name": "@solid-bricks/barcode",
  "version": "0.1.0",
  "description": "A solid-js component that renders barcodes",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c",
    "lint:ts": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FaberVitale/solid-bricks",
    "directory": "packages/barcode"
  },
  "license": "MIT",
  "keywords": [
    "barcode",
    "solid",
    "solidjs",
    "solid-js",
    "code128",
    "upc",
    "ean",
    "itf",
    "msi",
    "pharmacode"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "solid": "./dist/source/index.js",
